{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "import/extensions": ["error", { "js": "ignorePackages" }],
        "import/no-unresolved": 0,
        "linebreak-style": 0,
        "no-unused-vars": 0,
        "max-len": 0,
        "import/prefer-default-export": 0,
        "no-param-reassign": 0,
        "consistent-return": 0
    }
}
